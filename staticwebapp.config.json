{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/assets/*"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html"
    },
    {
      "route": "/console",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
